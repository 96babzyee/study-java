import java.util.Scanner;

public class ch02_5 {
	public static void main(String[] args) {
		System.out.print("정수 3개를 입력하시오>> ");
		
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		if((a+b)>c)
			System.out.println("삼각형이 됩니다.");
	}

}
