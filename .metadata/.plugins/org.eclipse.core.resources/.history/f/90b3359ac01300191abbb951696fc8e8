import java.util.Scanner;

public class ch02_4 {
	public static void main(String[] args) {
		System.out.print("정수 3개 입력 >>");
		
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt(); //30
		int b = sc.nextInt(); //20
		int c = sc.nextInt(); //25
		
		if(a>b) {
			if(a>c) System.out.println();
		}
	}

}
